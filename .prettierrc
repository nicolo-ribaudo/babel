{
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "tabWidth": 2,
  "printWidth": 80,
  "overrides": [{
    "files": [
      "**/codemods/*/src/**/*.{js,ts}",
      "**/codemods/*/test/**/*.{js,ts}",
      "**/packages/*/src/**/*.{js,ts}",
      "**/packages/*/test/**/*.{js,ts}",
      "**/eslint/*/src/**/*.{js,ts}",
      "**/eslint/*/test/**/*.{js,ts}"
    ],
    "options": {
      "trailingComma": "all"
    }
  }]
}
