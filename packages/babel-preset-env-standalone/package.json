{
  "name": "@babel/preset-env-standalone",
  "version": "7.7.4",
  "description": "Standalone build of babel-preset-env for use in non-Node.js environments.",
  "main": "babel-preset-env.js",
  "files": [
    "babel-preset-env.js",
    "babel-preset-env.min.js",
    "src"
  ],
  "scripts": {
    "prepublishOnly": "cd ../.. && make prepublish-build-preset-env-standalone"
  },
  "devDependencies": {
    "@babel/plugin-proposal-dynamic-import": "workspace:*",
    "@babel/plugin-proposal-json-strings": "workspace:*",
    "@babel/plugin-syntax-json-strings": "workspace:*",
    "@babel/plugin-syntax-top-level-await": "workspace:*",
    "@babel/plugin-transform-named-capturing-groups-regex": "workspace:*",
    "@babel/plugin-transform-new-target": "workspace:*",
    "@babel/preset-env": "workspace:*",
    "@babel/standalone": "workspace:*",
    "lodash": "^4.17.13"
  },
  "keywords": [
    "babel",
    "babel-preset-env",
    "babeljs",
    "6to5",
    "transpile",
    "preset",
    "standalone",
    "transpiler"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/babel/babel/issues"
  },
  "homepage": "https://github.com/babel/babel/packages/babel-preset-env-standalone#readme",
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/",
      "babel-preset-env.js"
    ]
  }
}
